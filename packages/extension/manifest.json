{
  "manifest_version": 2,
  "name": "a11y",
  "version": "0.1.0",
  "background": {
    "scripts": ["dist/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": "dist/content.js",
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "devtools_page": "dist/static/devtools.html",
  "permissions": ["<all_urls>"]
}
