// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Captions {
  id      Int    @id @default(autoincrement())
  url     String @unique
  caption String

  @@map("captions")
}
